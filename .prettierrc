{
  "plugins": ["prettier-plugin-tailwindcss"],
  "singleQuote": true,
  "semi": false,
  "tabWidth": 2,
  "printWidth": 80,
  "trailingComma": "es5",
  "overrides": [
    {
      "files": ["*.json", "**/*.json", "*.jsonc", "**/*.jsonc"],
      "options": {
        "trailingComma": "none"
      }
    }
  ]
}
